{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/packages/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/articles/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/destinations/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/locations/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/testimonials/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/wishlists/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/bookings/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/forums/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/reviews/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/payments/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/itineraries/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}